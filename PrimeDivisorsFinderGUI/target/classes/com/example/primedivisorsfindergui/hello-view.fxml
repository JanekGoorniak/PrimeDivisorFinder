<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.primedivisorsfindergui.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <HBox alignment="CENTER" prefHeight="33.0" prefWidth="200.0" spacing="10.0">
      <children>
         <Button mnemonicParsing="false" onAction="#onSaveButtonClick" text="Save" />
       <Button mnemonicParsing="false" onAction="#onLoadButtonClick" text="Load" />
          <Button onAction="#onFindButtonClick" text="Find!" />
      </children>
      <VBox.margin>
         <Insets />
      </VBox.margin>
      <opaqueInsets>
         <Insets />
      </opaqueInsets>
   </HBox>
   <TextField fx:id="input" onAction="#onFindButtonClick" promptText="enter a number" />
   <TextArea fx:id="output" prefHeight="200.0" prefWidth="200.0" promptText="no results" />
</VBox>
